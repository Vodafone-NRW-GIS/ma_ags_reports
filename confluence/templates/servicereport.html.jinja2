
<p>Auflistung aller ArcGIS-Server-Dienste in den konfigurierten Umgebungen sowie der darin verwendeten Datenbank-Tabellen. Für jeden Service ist angegeben, aus welcher .mxd-Datei über welche SDE-Verbindungsdatei er publiziert wurde.</p>


<ac:structured-macro ac:name="table-filter" ac:schema-version="1" ac:macro-id="0c41e665-43ee-4159-a8a5-ea0d9f8f453e">
<ac:parameter ac:name="fixedCols">1</ac:parameter>
<ac:parameter ac:name="inverse">false,false,false</ac:parameter>
<ac:parameter ac:name="sparkName">Sparkline</ac:parameter>
<ac:parameter ac:name="column">Service-Name,Umgebung</ac:parameter>
<ac:parameter ac:name="limitHeight">0</ac:parameter>
<ac:parameter ac:name="separator">Point (.)</ac:parameter>
<ac:parameter ac:name="labels">Service-Name&sbquo;Umgebung&sbquo;Globaler Filter</ac:parameter>
<ac:parameter ac:name="default">,,</ac:parameter>
<ac:parameter ac:name="cell-width">191,150,150</ac:parameter>
<ac:parameter ac:name="datepattern">dd M yy</ac:parameter>
<ac:parameter ac:name="id">1567408411183_1483867270</ac:parameter>
<ac:parameter ac:name="worklog">5|8|w d h m|w d h m</ac:parameter>
<ac:parameter ac:name="globalFilter">true</ac:parameter>
<ac:parameter ac:name="isOR">AND</ac:parameter>
<ac:parameter ac:name="order">1,2,0</ac:parameter>
<ac:rich-text-body>
<p class="auto-cursor-target"><br/></p>
<table width="100%">
<tbody>  
<tr>    
    <th>Service-Name</th>
    <th>Umgebung</th>
    <th>Verzeichnis</th>
    <th>DB</th>
    <th>Schema</th>
    <th>Tabelle</th>
    <th>SDE-Datei</th>
    <th>MXD-Datei</th>
    <th>Datum</th>
</tr>
 {% for record in data %}
    <tr>
         <td>
            {{record['svc_name']}}
        </td>
        <td>
           {{record['env']}}
        </td>
       <td>
           {{record['svc_folder']}}
        </td>
        <td>
           {{record['db']}}
        </td>
        <td>
           {{record['db_schema']}}
        </td>
          <td>
               {% if record['db'] == 'gis_pro' %}
              <ac:link>
                  <ri:page ri:content-title="Tabelle: {{record['db_table']}} (Schema: {{record['db_schema']}})" />
                   <ac:plain-text-link-body>
                    <![CDATA[{{record['db_table']}}]]>
                    </ac:plain-text-link-body>
                </ac:link>
              {% else %}
              <![CDATA[{{record['db_table']}}]]>
               {% endif %}
        </td>
       <td>
           {{record['sde']}}
        </td>
           <td>
           {{record['mxd']}}
        </td>
         <td>
           {{record['reference_date']}}
        </td>
    </tr>
 {% endfor %}
</tbody>
</table>
        </ac:rich-text-body></ac:structured-macro>